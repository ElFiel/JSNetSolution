#navbar-top.navbar.navbar-fixed-top
  .navbar-inner
    .container
      /%a.btn.btn-navbar{"data-toggle"=>"collapse", "data-target"=>"#nav-menu"}
      /  %span.icon-th-list
      /    Opciones de NavegaciÃ³n
      = link_to "JSNet Solution", home_path, :class=>"brand"        
      /#nav-menu.nav-collapse
      #nav-menu
        %ul.nav.nav-header
          %li
            = link_to home_path do
              %i.icon-home.icon-white 
              Home
          %li
            = link_to "/nosotros" do
              %i.icon-briefcase.icon-white
              Nosotros
          %li
            = link_to "/servicios" do
              %i.icon-wrench.icon-white
              Servicios
          //<li> <a href="/servicios"> <i class="icon-globe icon-white"></i> Servicios </a> </li>
          %li
            = link_to new_contact_path do
              %i.icon-envelope.icon-white
              Contacto
      /#nav-social.nav-collapse.pull-right
      #nav-social.pull-right
        %ul.nav.nav-header
          %li
            .txt_middle.collapse Siguenos :
          %li
            = link_to "#{LINK_FACEBOOK}", {:class => "btn_social"} do
              = image_tag "facebook_32.png"
          %li
            = link_to "#{LINK_TWITTER}", {:class => "btn_social"} do
              = image_tag "twitter_32.png"
.row-fluid
  .span12.well
    #logo.span3 
      = image_tag("logo_tmp1.png")
    #publi1.span9
      .row-fluid
        %p
      .row-fluid#publi1
        .pull-right.row-fluid#body-publi1
          = render partial: "layouts/publi1"
